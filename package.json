{
  "name": "@donkeyclip/motorcortex-ol",
  "version": "3.1.0",
  "description": "Openlayers library for MotorCortex",
  "main": "dist/motorcortex-ol.cjs.js",
  "module": "dist/motorcortex-ol.esm.js",
  "browser": "dist/motorcortex-ol.umd.js",
  "files": [
    "dist"
  ],
  "author": "Donkeyclip (donkeyclip.com) <opensource@donkeyclip.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-ol.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --hot --config ./demo/webpack.config.js --mode=development",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "openlayers",
    "ol",
    "map"
  ],
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "ol": "^6.13.0"
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": ">= 8 < 10"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.12",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-jsx": "7.17.12",
    "@babel/plugin-transform-react-jsx": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@donkeyclip/motorcortex": "9.3.1",
    "@donkeyclip/motorcortex-player": "2.10.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "babel-loader": "8.2.5",
    "browserslist": "4.20.3",
    "caniuse-lite": "1.0.30001341",
    "concurrently": "7.2.0",
    "core-js": "3.22.5",
    "css-loader": "^6.7.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "rollup": "2.74.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-terser": "7.0.2",
    "style-loader": "3.3.1",
    "tslib": "2.4.0",
    "webpack": "5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.9.0"
  }
}
